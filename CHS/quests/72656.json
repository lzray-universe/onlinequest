            "beginCondComb": "LOGIC_OR",
            "beginWay": "TALK_BEGIN_MANUAL",
            "heroTalk": "TALK_HERO_MAIN",
            "id": 7265601,
            "initDialog": 726560101,
            "nextRandomTalks": [],
