              ]
            },
            {
              "nodeId": 755090122,
              "roleType": "TALK_ROLE_PLAYER",
              "speakerId": null,
              "speakerName": "旅行者",
              "textMapHash": 2672351570,
              "text": "我举双手加派蒙的双手赞成！",
              "nextNodeIds": [
                755090123
              ]
            },
            {
              "nodeId": 755090114,
              "roleType": "TALK_ROLE_PLAYER",
