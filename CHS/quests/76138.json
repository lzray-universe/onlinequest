            "beginCondComb": "LOGIC_NONE",
            "beginWay": "TALK_BEGIN_MANUAL",
            "heroTalk": "TALK_HERO_LOCAL",
            "id": 7613801,
            "initDialog": 761380101,
            "nextRandomTalks": [],
