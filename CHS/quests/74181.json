            "beginCondComb": "LOGIC_NONE",
            "beginWay": "TALK_BEGIN_AUTO",
            "heroTalk": "TALK_HERO_MAIN",
            "id": 7418101,
            "initDialog": 741810101,
            "nextRandomTalks": [],
