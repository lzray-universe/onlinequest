            },
            {
              "nodeId": 708230510,
              "roleType": "TALK_ROLE_NPC",
              "speakerId": 2099,
              "speakerName": "阿宽",
              "textMapHash": 739593674,
              "text": "不行，我得快点再找一片能开工的地方…",
