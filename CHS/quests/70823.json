            },
            {
              "nodeId": 708230508,
              "roleType": "TALK_ROLE_NPC",
              "speakerId": 2099,
              "speakerName": "阿宽",
              "textMapHash": 3293019178,
              "text": "情况我知道了，谢谢你帮忙，这些收下吧。",
