            "beginCondComb": "LOGIC_NONE",
            "beginWay": "TALK_BEGIN_MANUAL",
            "heroTalk": "TALK_HERO_MAIN",
            "id": 7403304,
            "initDialog": 740330401,
            "nextRandomTalks": [],
            "nextTalks": [],
