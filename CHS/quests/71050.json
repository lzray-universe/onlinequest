            "beginCondComb": "LOGIC_NONE",
            "beginWay": "TALK_BEGIN_NONE",
            "heroTalk": "TALK_HERO_MAIN",
            "id": 7105007,
            "initDialog": 710500701,
            "nextRandomTalks": [],
