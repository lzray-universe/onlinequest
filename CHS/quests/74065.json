              "nodeId": 740650303,
              "roleType": "TALK_ROLE_NPC",
              "speakerId": 1005,
              "speakerName": "派蒙",
              "textMapHash": 2441283658,
              "text": "赶上一回？",
              "nextNodeIds": [
